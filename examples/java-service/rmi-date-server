# default: off
# description: rmi date example
service rmi-date-server
{
	disable	= no
	socket_type     = stream
	protocol 	= tcp
# setting this to no, makes the inherited channel be a socket instead of a
# serverSocket channel
	wait            = yes
	user            = root
	server          = /usr/bin/java
	server_args     = -Djava.security.policy=/home/amit/Documents/classes/cs555/lab/java-service/mysecurity.policy -Djava.rmi.server.hostname=127.0.0.1 -classpath /home/amit/Documents/classes/cs555/lab/java-service RMIDateServer
	log_on_success	= PID HOST USERID

}
